{
    "core":{
        "log":{
            "level":"INFO"
        },
        "cache":{
            "blackListLocalModules":["functions-io-core", "functions-io"]
        }
    },
    "registry":{
        "remote":{
            "url":"https://registry.npmjs.org"
        },
        "circuitBreakerTimeOut": 10000,
        "requestTimeout": 5000
    },
    "io":{
        "route":{
            "security.login":{"moduleName":"@functions-io-modules/security.token.generate", "moduleVersion":"*"}
        },
        "listGateway":{
            "http":{
                "moduleName":"@functions-io-modules/functions-io-modules-gateway-http",
                "enabled":true,
                "config":{
                    "web":{
                        "enabled":true,
                        "ecstatic":{
                            "root":"html",
                            "showDir":true,
                            "autoIndex":true
                        }
                    },
                    "http":{
                        "enabled":true,
                        "port":8080
                    },
                    "https":{
                        "enabled":true,
                        "port":8443,
                        "key":"key.pem",
                        "cert":"cert.pem",
                        "passphrase":"1234"
                    },
                    "listMiddlewareHTTP":["./handles/handleBody", "./handles/handleMessage", "./handles/handleHTML"],
                    "headers":{}
                }
            }
        },
        "listFilter":{
            "input":{
                "enabled":true,
                "moduleName":"@functions-io-modules/security.input.filter"
            },
            "authorization":{
                "enabled":false,
                "moduleName":"@functions-io-modules/security.authorization.filter",
                "config":{
                    "hasPermissionModuleName":"@functions-io-modules/security.haspermission.mongo",
                    "hasPermissionModuleVersion":"1.*",
                    "whiteList":["@functions-io-modules/security.token.generate"]
                }
            },
            "authentication":{
                "enabled":false,
                "moduleName":"@functions-io-modules/security.authentication.filter",
                "stage":"before",
                "config":{
                    "secretOrPrivateKey":"aaaa",
                    "algorithm":"HS256",
                    "issuer":"functions-io",
                    "whiteList":["@functions-io-modules/security.token.generate"]
                }
            }
        },
        "listDataSourceDriver":{
            "mongo":{
                "enabled":true,
                "moduleName":"@functions-io-modules/datasource.driver.mongo"
            },
            "mysql":{
                "enabled":false,
                "moduleName":"@functions-io-modules/datasource.driver.mysql"
            }
        }
    }
}